                <span list="f" format="({f})" style=" ">(h)</span><span style="font:7pt 'Times New Roman' !important;display:inline-block; width: 19px;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span><span free-text="31600"><!-- ngIf: editingEnabled && leaseFreeTexts[31600] != null && showRevertChanges --><!-- ngIf: !isPrerender --><span ng-if="!isPrerender" style="user-select: none;" class="ng-scope">&#8203;</span><!-- end ngIf: !isPrerender --><!-- ngIf: editingEnabled --><span ng-if="editingEnabled" ng-class="{'editable' : editingEnabled}" contenteditable="tempFalse" ng-model="leaseFreeTexts[31600]" bullet="false" section-id="31600" class="ng-pristine ng-untouched ng-valid ng-scope editable" tabindex="0" aria-invalid="false"><span style=" " class="ng-scope">Tenant shall not assign or sublease any of the Spaces without the consent of Landlord.</span><span style=" " class="ng-scope">&nbsp;</span><span style=" " class="ng-scope"> Landlord shall have the right to terminate Tenant’s parking rights with respect to any Spaces that Tenant desires to sublet or assign.</span></span><!-- end ngIf: editingEnabled --><!-- ngIf: !editingEnabled --></span>
