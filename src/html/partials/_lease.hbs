    <p style="margin-top:0pt; margin-bottom:12pt; text-align:justify; page-break-after:avoid; font-size:11pt">
        <span list="0" format="{0}." style=" font-weight:bold; text-transform:uppercase">12.</span><span style="font:7pt 'Times New Roman' !important;display:inline-block; width: 19px;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span><span free-text="18300"><!-- ngIf: editingEnabled && leaseFreeTexts[18300] != null && showRevertChanges --><!-- ngIf: !isPrerender --><span ng-if="!isPrerender" style="user-select: none;" class="ng-scope">&#8203;</span><!-- end ngIf: !isPrerender --><!-- ngIf: editingEnabled --><span ng-if="editingEnabled" ng-class="{'editable' : editingEnabled}" contenteditable="tempFalse" ng-model="leaseFreeTexts[18300]" bullet="false" section-id="18300" class="ng-pristine ng-untouched ng-valid ng-scope editable" tabindex="0" aria-invalid="false"><span style=" font-weight:bold" class="ng-scope">Notices.</span></span><!-- end ngIf: editingEnabled --><!-- ngIf: !editingEnabled --></span>
