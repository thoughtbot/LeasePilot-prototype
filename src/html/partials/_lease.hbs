    <p style="margin-top:0pt; margin-bottom:12pt; text-align:justify; page-break-after:avoid; widows:0; orphans:0; font-size:11pt">
        <span list="0" format="{0}." style=" font-weight:bold; text-transform:uppercase">9.</span><span style="font:7pt 'Times New Roman' !important;display:inline-block; width: 19px;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span><span free-text="17200"><!-- ngIf: editingEnabled && leaseFreeTexts[17200] != null && showRevertChanges --><!-- ngIf: !isPrerender --><span ng-if="!isPrerender" style="user-select: none;" class="ng-scope">&#8203;</span><!-- end ngIf: !isPrerender --><!-- ngIf: editingEnabled --><span ng-if="editingEnabled" ng-class="{'editable' : editingEnabled}" contenteditable="tempFalse" ng-model="leaseFreeTexts[17200]" bullet="false" section-id="17200" class="ng-pristine ng-untouched ng-valid ng-scope editable" tabindex="0" aria-invalid="false"><span style=" font-weight:bold" class="ng-scope">Repairs and </span><span style=" font-weight:bold" class="ng-scope">Maintenance</span><span style=" font-weight:bold" class="ng-scope">.</span></span><!-- end ngIf: editingEnabled --><!-- ngIf: !editingEnabled --></span>
