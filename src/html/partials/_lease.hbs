                <span list="f" format="({f})" style=" ">(c)</span><span style="font:7pt 'Times New Roman' !important;display:inline-block; width: 19px;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span><span free-text="31100"><!-- ngIf: editingEnabled && leaseFreeTexts[31100] != null && showRevertChanges --><!-- ngIf: !isPrerender --><span ng-if="!isPrerender" style="user-select: none;" class="ng-scope">&#8203;</span><!-- end ngIf: !isPrerender --><!-- ngIf: editingEnabled --><span ng-if="editingEnabled" ng-class="{'editable' : editingEnabled}" contenteditable="tempFalse" ng-model="leaseFreeTexts[31100]" bullet="false" section-id="31100" class="ng-pristine ng-untouched ng-valid ng-scope editable" tabindex="0" aria-invalid="false"><span style=" " class="ng-scope">Except for particular spaces and areas designated by Landlord or the Operator for reserved parking, all parking in the Garage shall be on an unreserved, first-come, first-served basis.</span></span><!-- end ngIf: editingEnabled --><!-- ngIf: !editingEnabled --></span>
