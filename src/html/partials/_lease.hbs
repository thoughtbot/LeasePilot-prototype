        <span list="0" format="{0}." style=" font-weight:bold; text-transform:uppercase">17.</span><span style="font:7pt 'Times New Roman' !important;display:inline-block; width: 19px;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span><span free-text="22700"><!-- ngIf: editingEnabled && leaseFreeTexts[22700] != null && showRevertChanges --><!-- ngIf: !isPrerender --><span ng-if="!isPrerender" style="user-select: none;" class="ng-scope">&#8203;</span><!-- end ngIf: !isPrerender --><!-- ngIf: editingEnabled --><span ng-if="editingEnabled" ng-class="{'editable' : editingEnabled}" contenteditable="tempFalse" ng-model="leaseFreeTexts[22700]" bullet="false" section-id="22700" class="ng-pristine ng-untouched ng-valid ng-scope editable" tabindex="0" aria-invalid="false"><span style=" font-weight:bold" class="ng-scope">Limitation of Liability.</span></span><!-- end ngIf: editingEnabled --><!-- ngIf: !editingEnabled --></span>
