{{#> layout }} {{> _header}}

<section class="center flex justify-between mt4">
  <div class="bg-black-05 m-compare__item pa3">
    <div class="bg-white center pa5">
      {{> _lease }}
    </div>
  </div>


  <div class="m-compare__item ph3 relative" data-compare-container>
    {{> _compare-header num="1" reviewed="0" prev="" next="compare-ignore"}}

    <div class="b--lightest-blue bt bw2 absolute w-90" style="top: 400px;" data-compare-card="request-1">
      <div class="b--black-10 ba pa3 relative">
        {{> _compare-card-meta num="1"}}

        {{#diffColor "addition"}}
        <div data-compare-copy="request-1">
          <p class="georgia lh-copy dark-gray">
            <span class="underline ml4">7.01 Building Services.</span> Landlord shall furnish Tenant with the following services: (a) water for use in the Base Building <mark class="mark-color black relative" data-diff>restrooms and any
            restrooms and kitchens located with the Premises;</mark> (b) customary heat and air conditioning in season during Building Service <mark class="mark-color black relative" data-diff>Hours for first class office
            buildings</mark> in the <mark class="mark-color black relative" data-diff>City of Boston</mark>;</p>
        </div>
        {{/diffColor}}

      </div>

      {{> _compare-card-actions num="1"}}
    </div>
  </div>
</section>

{{> _compare-action-hover-btns}}
{{/ layout }}
